<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#593E25"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageViewFlo"
        android:layout_width="330dp"
        android:layout_height="276dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="16dp"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/fotofloo" />

    <TextView
        android:id="@+id/textViewAboutMe"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="#BF946F"
        android:backgroundTint="#BF946F"
        android:padding="20dp"
        android:text="Soy Florencia"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageViewFlo" />

    <TextView
        android:id="@+id/textViewExpDevelop"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="#D9AF8B"
        android:drawableLeft="@drawable/developer"
        android:padding="20dp"
        android:text="Experiencia Develop"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/textViewExpNurse"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textViewExpNurse"
        app:layout_constraintTop_toBottomOf="@+id/textViewExpNurse" />

    <TextView
        android:id="@+id/textViewMoti"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="1dp"
        android:background="#F2CEAE"
        android:drawableLeft="@drawable/moti"
        android:padding="20dp"
        android:text="MotivaciÃ³n"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/textViewExpDevelop"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/textViewExpDevelop"
        app:layout_constraintTop_toBottomOf="@+id/textViewExpDevelop"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textViewExpNurse"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="#D9AF8B"
        android:drawableLeft="@drawable/nurse"
        android:paddingLeft="20dp"
        android:paddingTop="20dp"
        android:paddingRight="20dp"
        android:text="TextView"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textViewAboutMe"
        app:layout_constraintTop_toBottomOf="@+id/textViewAboutMe" />

    <ImageButton
        android:id="@+id/buttonContact"
        android:layout_width="62dp"
        android:layout_height="69dp"
        android:contentDescription="FotoFlorencia"
        app:layout_constraintBottom_toBottomOf="@+id/imageViewFlo"
        app:layout_constraintEnd_toEndOf="@+id/imageViewFlo"
        app:layout_constraintHorizontal_bias="0.42"
        app:layout_constraintStart_toEndOf="@+id/imageViewFlo"
        app:layout_constraintTop_toTopOf="@+id/imageViewFlo"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/share" />

</androidx.constraintlayout.widget.ConstraintLayout>